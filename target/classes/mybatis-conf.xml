			<!--<property name="dialect" value="mysql"/>-->
		<!--</plugin>-->
		<!--<plugin interceptor="com.chaoxing.modules.mybatis.page.ResultSetInterceptor">-->
		<!--</plugin>-->
	<!--</plugins>-->
</configuration>